{
  "name": "bug-training-game",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "prestart": "node ./scripts/buffer-polyfill.js",
    "start": "cross-env NODE_ENV=production node -r ./scripts/buffer-polyfill.js backend/server.js",
    "dev": "nodemon -r ./scripts/buffer-polyfill.js backend/server.js",
    "test": "jest --watchAll --verbose",
    "build": "npm install && node ./scripts/build.js",
    "postbuild": "cd build && npm install --production"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.12.0",
    "mongoose": "^8.8.4",
    "aws-sdk": "^2.1550.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "jest": "^29.0.0",
    "supertest": "^6.0.0"
  },
  "overrides": {
    "buffer-equal-constant-time": "npm:@noble/hashes@^1.3.2"
  }
}
